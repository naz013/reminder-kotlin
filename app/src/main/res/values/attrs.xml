<?xml version="1.0" encoding="UTF-8"?>
<resources>

    <attr name="colorSecondary12" format="color" />
    <attr name="colorSecondary50" format="color" />
    <attr name="colorOnSecondary50" format="color" />

    <attr name="alphaEmphasisHigh" format="float"/>
    <attr name="alphaEmphasisMedium" format="float"/>
    <attr name="alphaEmphasisDisabled" format="float"/>

    <declare-styleable name="UiSelectorView">
        <attr name="selector_canShowDialog" format="boolean" />
        <attr name="selector_leftButtonBackground" format="color" />
        <attr name="selector_rightButtonBackground" format="color" />
        <attr name="selector_textBackground" format="color" />
        <attr name="selector_emptyText" format="string" />
    </declare-styleable>

    <declare-styleable name="UiCheckableImageView">
        <attr name="civ_checked" format="boolean" />
        <attr name="civ_checkedIcon" format="reference" />
        <attr name="civ_uncheckedIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="PrefsView">
        <attr name="prefs_primary_text" format="string" />
        <attr name="prefs_primary_text_color" format="color" />
        <attr name="prefs_secondary_text_on" format="string" />
        <attr name="prefs_secondary_text_off" format="string" />
        <attr name="prefs_secondary_text" format="string" />
        <attr name="prefs_value_text" format="string" />
        <attr name="prefs_divider_top" format="boolean" />
        <attr name="prefs_divider_bottom" format="boolean" />
        <attr name="prefs_checked" format="boolean" />
        <attr name="prefs_pro" format="boolean" />
        <attr name="prefs_isTest" format="boolean" />
        <attr name="prefs_view_resource" format="reference" />
        <attr name="prefs_icon" format="reference" />
        <attr name="prefs_type" format="enum">
            <enum name="check" value="0" />
            <enum name="check_switch" value="1" />
            <enum name="view" value="2" />
            <enum name="text" value="3" />
            <enum name="none" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="DateTimeView">
        <attr name="date_text_size" format="dimension" />
        <attr name="time_text_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RepeatView">
        <attr name="repeatType" format="enum">
            <enum name="seconds" value="0" />
            <enum name="minutes" value="1" />
            <enum name="hours" value="2" />
            <enum name="days" value="3" />
            <enum name="weeks" value="4" />
            <enum name="months" value="5" />
        </attr>
        <attr name="isLocked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BeforePickerView">
        <attr name="before_type" format="enum">
            <enum name="seconds" value="0" />
            <enum name="minutes" value="1" />
            <enum name="hours" value="2" />
            <enum name="days" value="3" />
            <enum name="weeks" value="4" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ActionView" />
</resources>